<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 256 256" width="256" height="256">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e40af;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#60a5fa;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="bookGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#10b981;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#059669;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="screenGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1f2937;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#374151;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Fondo circular -->
  <circle cx="128" cy="128" r="120" fill="url(#bgGradient)" stroke="#1e40af" stroke-width="4"/>
  
  <!-- Libro/Manual base -->
  <rect x="50" y="90" width="80" height="100" rx="6" fill="url(#bookGradient)" stroke="#059669" stroke-width="2"/>
  
  <!-- Páginas del libro -->
  <rect x="55" y="95" width="70" height="90" rx="4" fill="#ffffff" stroke="#e5e7eb" stroke-width="1"/>
  
  <!-- Líneas de texto en el libro -->
  <rect x="62" y="105" width="50" height="2" fill="#374151" opacity="0.8"/>
  <rect x="62" y="112" width="45" height="2" fill="#374151" opacity="0.6"/>
  <rect x="62" y="119" width="55" height="2" fill="#374151" opacity="0.7"/>
  <rect x="62" y="126" width="40" height="2" fill="#374151" opacity="0.8"/>
  <rect x="62" y="133" width="48" height="2" fill="#374151" opacity="0.6"/>
  <rect x="62" y="140" width="52" height="2" fill="#374151" opacity="0.7"/>
  
  <!-- Monitor/Pantalla -->
  <rect x="140" y="80" width="70" height="50" rx="4" fill="url(#screenGradient)" stroke="#374151" stroke-width="2"/>
  <rect x="145" y="85" width="60" height="35" rx="2" fill="#000000"/>
  
  <!-- Contenido de la pantalla - gráficos educativos -->
  <rect x="150" y="90" width="15" height="8" fill="#3b82f6" opacity="0.8"/>
  <rect x="150" y="100" width="25" height="8" fill="#10b981" opacity="0.8"/>
  <rect x="150" y="110" width="20" height="8" fill="#f59e0b" opacity="0.8"/>
  
  <!-- Iconos de datos -->
  <circle cx="185" cy="94" r="3" fill="#ffffff" opacity="0.9"/>
  <circle cx="185" cy="104" r="3" fill="#ffffff" opacity="0.7"/>
  <circle cx="185" cy="114" r="3" fill="#ffffff" opacity="0.8"/>
  
  <!-- Base del monitor -->
  <rect x="170" y="130" width="10" height="15" fill="#6b7280"/>
  <rect x="160" y="145" width="30" height="4" rx="2" fill="#6b7280"/>
  
  <!-- Engranajes (gestión/administración) -->
  <g transform="translate(180, 160)">
    <circle cx="0" cy="0" r="12" fill="#f59e0b" stroke="#d97706" stroke-width="2"/>
    <circle cx="0" cy="0" r="6" fill="#ffffff"/>
    <rect x="-2" y="-8" width="4" height="4" fill="#d97706"/>
    <rect x="-2" y="4" width="4" height="4" fill="#d97706"/>
    <rect x="-8" y="-2" width="4" height="4" fill="#d97706"/>
    <rect x="4" y="-2" width="4" height="4" fill="#d97706"/>
  </g>
  
  <g transform="translate(200, 175)">
    <circle cx="0" cy="0" r="8" fill="#ef4444" stroke="#dc2626" stroke-width="1.5"/>
    <circle cx="0" cy="0" r="4" fill="#ffffff"/>
    <rect x="-1" y="-6" width="2" height="2" fill="#dc2626"/>
    <rect x="-1" y="4" width="2" height="2" fill="#dc2626"/>
    <rect x="-6" y="-1" width="2" height="2" fill="#dc2626"/>
    <rect x="4" y="-1" width="2" height="2" fill="#dc2626"/>
  </g>
  
  <!-- Icono de red/conectividad -->
  <g transform="translate(70, 50)">
    <circle cx="0" cy="0" r="4" fill="#ffffff" opacity="0.9"/>
    <circle cx="15" cy="0" r="4" fill="#ffffff" opacity="0.9"/>
    <circle cx="30" cy="0" r="4" fill="#ffffff" opacity="0.9"/>
    <line x1="4" y1="0" x2="11" y2="0" stroke="#ffffff" stroke-width="2" opacity="0.8"/>
    <line x1="19" y1="0" x2="26" y2="0" stroke="#ffffff" stroke-width="2" opacity="0.8"/>
  </g>
  
  <!-- Texto AIP en la parte inferior -->
  <text x="128" y="220" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#ffffff" opacity="0.9">AIP</text>
</svg>